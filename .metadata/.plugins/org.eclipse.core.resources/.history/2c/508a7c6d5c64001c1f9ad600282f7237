package com.day3;

import java.util.Scanner;

public class Test1 {

	public static void main(String[] args) {
		
		//반복문(for,while,do~while)
		
		Scanner sc = new Scanner(System.in);
		
		int dan;
		
		System.out.println("단 입력?");
		dan = sc.nextInt();
		
		for(int i = 1; i <= 9000000; i++) {
		System.out.println(dan + "*" + i + "=" + (dan * i));

		
		}

	}

}
