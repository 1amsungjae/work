package com.day5;

public class Test2 {

	public static void main(String[] args) {
		
		
		int[] num = {37,26,19,12,7};
		
		int i,j,temp;
		
		System.out.println("Source Date : ");
		for(i=0;i<num.length;i++) {
			System.out.printf("%4d",num[i]);
		}
		System.out.println();
		
		
		
		System.out.println("Source Date : ");
		for(int s : num) {
			System.out.printf("%4d",s);
		}
		System.out.println();


	}

}
