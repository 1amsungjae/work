package com.day1;

public class Test3 {

	public static void main(String[] args) {
		
		int r = 10;
		float area,length; //실수(소숫점)
		
		area = r*r*3.14f;
		length = r*2*3.14f;
		
		System.out.println(area);
		System.out.println(length);
		
		System.out.println("반지금 : " + r + ", 면적 :  " + area);
		System.out.printf("반지름 : %d, 면적 %2f",r,area);
				
	}

}
